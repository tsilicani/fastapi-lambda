    "cSpell.words": [
        "Arepa",
        "ASGI",
        "astack",
        "asyncio",
        "Cabe",
        "Chimichang",
        "conftest",
        "Deadpoolio",
        "dunder",
        "exceptiongroups",
        "excgroups",
        "fastapi",
        "fastapifn",
        "fastfn",
        "globalns",
        "htmlcov",
        "ipykernel",
        "isort",
        "jsonable",
        "localstack",
        "mccabe",
        "Morty",
        "multiparts",
        "mutablecopy",
        "mypy",
        "nocover",
        "nonable",
        "norecursedirs",
        "parameterless",
        "poplist",
        "Pylance",
        "pyright",
        "pytest",
        "PYTHONPATH",
        "Ramírez",
